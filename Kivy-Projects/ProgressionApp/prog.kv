#:kivy 1.11
<CreateAcc>:
    name: "create"
    username:username
    email:email
    passw:passw
    loginf:loginf
    create:create
    FloatLayout:
        cols:1

        FloatLayout:
            Label:
                text:"Username: "
            TextInput:
                id:username
                text: "Username:"

            Label:
                text:"E-mail"

            TextInput:
                id:email
                text: "E-mail:"
            
            Label:
                text:"Password: "
            
            TextInput:
                id:passw
            Button:
                id:loginf
                text:"Already Have an Account?"

            Button:
                id:create
                text:"Create Account"
            


<Login>:
    name: "login"
    email:email
    passw:passw
    createf:createf
    login:login
    FloatLayout:
        cols:1
        FloatLayout:
            TextInput:
                id:email
                text: "Email:"
            TextInput:
                id:passw
                text:"Password:"

            Button:
                id:createf
                text:"Don't have an Account?"
            
            Button:
                id:login
                text:"Login"




<Profile>:
    name: "profile"